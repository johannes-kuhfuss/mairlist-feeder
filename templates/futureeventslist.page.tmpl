{{ define "futureeventslist.page.tmpl" }}

{{ template "header" .}}

   <div class="container-fluid py-5">
        <div class="row">
            <div class="col">
                <table class="table table-striped table-sm">
                    <thead>
                        <tr>
                          <th scope="col">Event Id</th>
                          <th scope="col">Title</th>
                          <th scope="col">Start Date</th>
                          <th scope="col">Start Time</th>
                          <th scope="col">End Time</th>
                          <th scope="col">File Uplodad in calCms</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{ range .events }}
                        <tr>
                          <td>{{ .EventId }}</td>
                          <td>{{ .Title }}</td>
                          <td>{{ .StartDate }}</td>
                          <td>{{ .StartTime }}</td>
                          <td>{{ .EndTime }}</td>
                          <td>{{ .FileAvail }}</td>
                        </tr>
                        {{ end }}
                    </tbody>
                </table>
            </div>
        </div>
    </div>

{{ template "footer" .}}

{{ end }}
